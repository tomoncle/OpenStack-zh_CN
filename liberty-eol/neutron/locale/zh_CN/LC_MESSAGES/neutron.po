#, python-format
msgid ""
"\n"
"Command: %(cmd)s\n"
"Exit code: %(code)s\n"
"Stdin: %(stdin)s\n"
"Stdout: %(stdout)s\n"
"Stderr: %(stderr)s"
msgstr ""
"\n"
"命令：%(cmd)s\n"
"退出代码：%(code)s\n"
"标准输入：%(stdin)s\n"
"标准输出：%(stdout)s\n"
