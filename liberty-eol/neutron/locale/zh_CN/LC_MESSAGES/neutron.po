#, python-format
msgid "%(driver)s: Internal driver error."
msgstr "%(driver)s: 内部驱动错误。"

#, python-format
msgid "%(err_msg)s"
msgstr "%(err_msg)s"

#, python-format
msgid "%(id)s is not a valid %(type)s identifier"
msgstr "%(id)s 不是有效的 %(type)s 标识"

