msgid "Bad %(resource)s request: %(msg)s"
msgstr "%(resource)s 请求不正确：%(msg)s"

#, python-format
msgid ""
"Bad prefix or mac format for generating IPv6 address by EUI-64: %(prefix)s, "
"%(mac)s:"
