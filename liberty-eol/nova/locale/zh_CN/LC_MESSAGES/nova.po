msgid "%s is not supported."
msgstr "不支持%s"

#, python-format
msgid "%s must be either 'MANUAL' or 'AUTO'."
msgstr "%s 必须是'MANUAL' 或者 'AUTO'。"

#, python-format
msgid "'%(other)s' should be an instance of '%(cls)s'"
msgstr "'%(other)s'应该是'%(cls)s'的实例"

